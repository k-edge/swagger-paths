{
  "name": "@k-edge/swagger-paths",
  "version": "1.0.7",
  "description": "Match paths from a swagger file",
  "main": "index.js",
  "scripts": {
    "test": "mocha 'test/**/*.test.js' || false",
    "lint": "eslint --cache '.eslintrc.js' '**/*js'",
    "lintfix": "eslint --cache '.eslintrc.js' '**/*js' --fix",
    "coverage": "nyc --reporter=text npm run test",
    "coverage-report": "nyc --reporter=html --reporter=text npm run test",
    "build": "npm run lint && npm run coverage"
  },
  "author": {
    "name": "Pier Fumagalli",
    "email": "pier.fumagalli@koerber.digital"
  },
  "homepage": "https://github.com/k-edge/swagger-paths#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k-edge/swagger-paths.git"
  },
  "bugs": {
    "url": "https://github.com/k-edge/swagger-paths/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0"
  }
}
